library(readr)
library(readr)
library(readr)
library(rcompanion)
library(nortest)
dane <- read.csv2("c02_B2.csv")
dane$id <- NULL
# Przedzialy
delta <- 0.03
breaks <- seq(from = min(dane$Ti),
to = max(dane$Ti) + delta, by = delta)
dane_cut <- cut(dane$Ti, breaks = breaks, include.lowest = TRUE)
ni <- table(dane_cut)
sum(ni) == length(dane$Ti)
h1 <- hist(x = dane$Ti, main = "Histogram okresu jednego wahnięcia",
ylab = "Częstotliwość wystąpienia",
xlab = "Okres wahnięcia",
col = "pink",
breaks = breaks,
yaxt = "n")
axis(side = 2, at = seq(0, 50, by = 5))
plotNormalHistogram(
dane$Ti,
prob = TRUE,
col = "pink",
main = "Histogram z krzywą normalną",
linecol = "red",
lwd = 2,
length = 1000,
breaks = breaks,
ylab = "Gęstość prawdopodobieństwa",
xlab = "Okres wahnięcia"
)
d <- density(dane$Ti)
lines(d, col = "purple", lwd = 2)
# Dane
x <- dane$Ti
# Histogram bez normalizacji
h <- hist(x, prob=FALSE, breaks = breaks, col = "pink", main = "Histogram z przeskalowaną krzywą Gaussa", xlab = "Okres wahnięcia")
# Parametry rozkładu
m <- mean(x)
s <- sd(x)
# Skalowanie: liczba obserwacji * szerokość binów
dx <- diff(h$mids[1:2])
n <- length(x)
# Generowanie przeskalowanej krzywej
xfit <- seq(min(x), max(x), length = 1000)
yfit <- n * dx * dnorm(xfit, mean = m, sd = s)
# Dodanie krzywej
lines(xfit, yfit, col = "red", lwd = 2)
# Średnia i odchylenie standardowe
mean_x <- mean(dane$Ti)
sd_x <- sd(dane$Ti)
# Pierwszy przedział
lower1 <- mean_x - sd_x
upper1 <- mean_x + sd_x
# Drugi przedział
lower2 <- mean_x - 2*sd_x
upper2 <- mean_x + 2*sd_x
# Liczba wyników poza przedziałem x ± s
outside1 <- sum(dane$Ti < lower1 | dane$Ti > upper1)
# Liczba wyników poza przedziałem x ± 2s
outside2 <- sum(dane$Ti < lower2 | dane$Ti > upper2)
outside1
outside2
pearson.test(dane$mTi)
shapiro.test(dane$Ti)
